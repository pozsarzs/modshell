<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <ProjectOptions>
    <Version Value="12"/>
    <General>
      <SessionStorage Value="InProjectDir"/>
      <Title Value="XModShell"/>
      <Scaled Value="True"/>
      <ResourceType Value="res"/>
      <UseXPManifest Value="True"/>
      <XPManifest>
        <DpiAware Value="True"/>
        <TextName Value="Pozsar_Zsolt.ModShell.ModShell"/>
        <TextDesc Value="Command-driven scriptable Modbus utility with GUI"/>
      </XPManifest>
      <Icon Value="0"/>
