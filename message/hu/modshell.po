"  set con? dev? pro?\\015\n"
"  set prj PROJECT_NAME\\015\n"
"  ?: [0-7]"
msgstr ""
"set dev? net ESZKÖZ PORT\\015\n"
"  set dev? ser ESZKÖZ SEBESSÉG ADATBIT PARITÁS STOPBIT\\015\n"
"  set pro? ascii|rtu UID\\015\n"
"  set pro? tcp IP_CÍM\\015\n"
"  set con? dev? pro?\\015\n"
"  set prj PROJEKTNÉV\\015\n"
"  ?: [0-7]"

