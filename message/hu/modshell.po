#: modshell:err00
msgid "No such command!"
msgstr "Nincs ilyen parancs!"

#: modshell:err01
msgid "Device number must be 0-7!"
msgstr "Az eszköz számának 0-7-nek kell lennie!"

#: modshell:err02
msgid "Protocol number must be 0-7!"
msgstr "A protokoll számának 0-7-nek kell lennie!"

#: modshell:err03
msgid "Connection number must be 0-7!"
msgstr "A kapcsolat számának 0-7-nek kell lennie!"

#: modshell:err04
msgid "IP address is not valid!"
msgstr "Az IP -cím nem érvényes!"

#: modshell:err05
msgid "Parameter(s) required!"
msgstr "Paraméterek szükséges(ek)!"

#: modshell:err06
msgid "UID must be 1-247!"
msgstr "Az UID-nek 1-247-nek kell lennie!"

#: modshell:err07
msgid "Cannot export command line history to "
msgstr "Nem lehet exportálni a parancssori előzményeket ebbe: "

#: modshell:err08
msgid "Cannot save settings to "
msgstr "Nem lehet menteni a beállításokat ebbe: "

#: modshell:err09
msgid "Cannot load settings from "
msgstr "Nem lehet betölteni a beállításokat ebből: "

#: modshell:err10
msgid "Cannot export register content to "
msgstr "Nem lehet exportálni a regisztertartalmat ebbe: "

#: modshell:err11
msgid "Cannot import register content from "
msgstr "Nem lehet importálni a regisztertartalmat ebből: "

#: modshell:err12
msgid "Cannot save register content to "
msgstr "Nem mentheti a regisztertartalmat ebbe: "

#: modshell:err13
msgid "Cannot load register content from "
msgstr "Nem lehet betölteni a regisztertartalmat ebből: "

#: modshell:err14
msgid "Illegal character in the project name!"
msgstr "Nem engedélyezett karakter a projekt nevében!"

#: modshell:err15
msgid "Illegal character in the variable name!"
msgstr "Nem engedélyezett karakter a változó nevében!"

#: modshell:err16
msgid "Cannot define more variable!"
msgstr "Nem lehet létrehozni a változót!"

#: modshell:err17
msgid "There is already a variable with that name"
msgstr "Van már egy változó ezzel a névvel"

#: modshell:err18
msgid "Cannot initialize serial port!"
msgstr "Nem lehet inicializálni a soros portot!"

#: modshell:des00
msgid "       copy one or more register between two connections"
msgstr "       egy vagy több regiszter másolása két kapcsolat között"

#: modshell:des01
msgid "F10    exit"
msgstr "F10    kilépés"

#: modshell:des02
msgid "ALT-G  show device, protocol, connection or project name"
msgstr "Alt-G  eszköz, protokoll, kapcsolat vagy projektnév megjelenítése"

#: modshell:des03
msgid "F1     show description or usage of the commands"
msgstr "F1     parancs leírás vagy használat megjelenítése"

#: modshell:des04
msgid "ALT-L  set value of a buffer registers"
msgstr "Alt-L  regisztertartalom beállítása"

#: modshell:des05
msgid "ALT-P  print message, value of the variable and register"
msgstr "ALT-P  üzenet, a változó vagy regisztertartalom kiírása"

#: modshell:des06
msgid "ALT-R  read one or more remote registers to buffer"
msgstr "Alt-R  adat olvasása egy vagy több távoli regiszterből a pufferbe"

#: modshell:des07
msgid "ALT-T  reset device, protocol, connection or project name"
msgstr "ALT-T  eszköz, protokoll, kapcsolat vagy projektnév törlése"

#: modshell:des08
msgid "ALT-S  set device, protocol, connection or project name"
msgstr "Alt-S  eszköz, protokoll, kapcsolat vagy projektnév beállítása"

#: modshell:des09
msgid "       show system date and time"
msgstr "       dátum és idő megjelenítése"

#: modshell:des10
msgid "       show version and build information of this program"
msgstr "       változat és fordítási információk a programról"

#: modshell:des11
msgid "ALT-W  write data from buffer to one or more remote registers"
msgstr "Alt-W  adat írása egy vagy több pufferből a távoli regiszterbe"

#: modshell:des12
msgid "F8     clear screen"
msgstr "F8     képernyőtörlés"

#: modshell:des13
msgid "F2     save settings of device, protocol and connection"
msgstr "F2     Eszköz-, protokoll- és kapcsolatbeállítások mentése"

#: modshell:des14
msgid "F3     load settings of device, protocol and connection"
msgstr "F3    eszköz-, protokoll- és kapcsolatbeállítások betöltése"

#: modshell:des15
msgid "ALT-E  export value of the one or more buffer registers"
msgstr "ALT-E  egy vagy több regisztertartalom exportálása"

#: modshell:des16
msgid "       export command line history to make a script easily"
msgstr "       parancssori előzmények exportálása"

#: modshell:des17
msgid "       convert value between different numeral systems"
msgstr "       konvertálás a különböző számrendszerek között"

#: modshell:des18
msgid "F4     save all registers"
msgstr "F5     összes regisztertartalom mentése"

#: modshell:des19
msgid "F5     load all registers"
msgstr "F5     összes regisztertartalom betöltése"

#: modshell:des20
msgid "       define new variable and assign value"
msgstr "       új változó definiálás és értékadás"

#: modshell:des21
msgid "       set foreground and background color in full screen mode"
msgstr "       előtér- és a háttérszín beállítása (fullscreen)"

#: modshell:des22
msgid "ALT-I  import value of the one or more buffer registers"
msgstr "ALT-I  egy vagy több regisztertartalom importálása"

